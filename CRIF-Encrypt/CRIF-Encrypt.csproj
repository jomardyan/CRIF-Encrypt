    <RootNamespace>CRIF_Encrypt</RootNamespace>
    <ApplicationIcon>database.ico</ApplicationIcon>
    <Version>1.0.2</Version>
    <Authors>Hayk Jomardyan</Authors>
    <Copyright>JHC</Copyright>
    <PackageLicenseExpression />
